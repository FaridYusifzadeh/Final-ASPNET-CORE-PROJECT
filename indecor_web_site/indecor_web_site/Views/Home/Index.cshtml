<!--Product Area Start-->
<div class="product-area pt-20 pb-35">
    <div class="container">
        <div class="row">
            <div class="col-12">
                <!--Section Title 2 Start-->
                <div class="section-title2">
                    <h2>Our Products</h2>
                    <!--Product Tab Menu Start-->
                    <div class="product-tab-menu">
                        <ul class="nav nav-tabs" id="myTab" role="tablist">
                            @foreach (var item in Model.Product_Categories)
                            {
                                <li><a class="navbar-link @item.Active" data-toggle="tab" aria-selected="true" href="#tab-@item.Id">@item.Name</a></li>
                            }


                        </ul>
                    </div>
                    <!--Product Tab Menu End-->
                </div>
                <!--Section Title 2 End-->
            </div>
        </div>
        <div class="row">
            <div class="col-12">
                <div class="tab-content" id="myTabContent">
                    @foreach (var item in Model.Product_Categories)
                    {
                        <div class="tab-pane fade show @item.Active" id="tab-@item.Id" role="tabpanel">
                            <div class="row">
                                <div class="product-slider owl-carousel">
                                    @{ var size = (Model.Products.Count() % 2 == 0 ? Model.Products.Count() / 2 : Model.Products.Count() / 2 + 1); }

                                    @for (int i = 0, j = 0; i < size; i++, j += 2)
                                    {
                                        <div class="col-md-12">
                                            @foreach (var prod in Model.Products.Skip(j).Take(2))
                                            {
                                                <div class="single-product-area mb-40">
                                                    <div class="product-img img-full">
                                                        <a href="single-product.html">
                                                            <img class="first-img" src="img/product/@prod.Image" alt="">
                                                        </a>
                                                        <span class="sticker">New</span>
                                                        <div class="product-action">
                                                            <ul>
                                                                <li><a href="cart.html" title="Add To Cart"><span class="icon_bag_alt"></span></a></li>
                                                                <li><a href="wishlist.html" title="Add to Wish List"><span class=" icon_heart_alt "></span></a></li>
                                                                <li><a href="compare.html" title="Compare this Product"><span class="icon_datareport"></span></a></li>
                                                            </ul>
                                                        </div>
                                                        <ul class="product-quickview">
                                                            <li><a href="#open-modal" data-toggle="modal" title="Quick View"><span class="icon_search"></span></a></li>
                                                        </ul>
                                                    </div>
                                                    <div class="product-content">
                                                        <h4><a href="single-product.html">@prod.Name</a></h4>
                                                        <div class="product-price">
                                                            <span class="now-price">$@prod.Price</span>
                                                        </div>
                                                    </div>
                                                </div>
                                            }
                                        </div>
                                    }
                                </div>
                            </div>
                        </div>
                    }


                </div>
            </div>
        </div>
    </div>
</div>
<!--Product Area End-->
