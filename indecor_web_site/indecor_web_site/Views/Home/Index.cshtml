        <div class="row">
            <div class="col-12">
                <!--Section Title 2 Start-->
                <div class="section-title2">
                    <h2>from our blogs</h2>
                </div>
                <!--Section Title 2 End-->
            </div>
        </div>
        <div class="row">
            <div class="blog-slider owl-carousel">
                @foreach (var ourBlog in Model.FromOurBlogs)
                {
                    <div class="col-md-12">
                        <!--Single Blog Start-->
                        <div class="single-blog">
                            <div class="blog-img img-full">
                                <a href="single-blog.html"><img src="~/img/blog/small/@ourBlog.Image" alt=""><span class="icon-view"></span></a>
